package com.code.Practice;

public class Switchcase {

	public static void main(String[] args) {

		
		
		int day =3;
		switch (day) {
		case 1:
		    System.out.println("yes0");
		case 2:
			System.out.println("today");
		case 3:
		System.out.println("tomo");
		break;
		default:
			System.out.println("exit form the case");
			
		}
		
	}

}
