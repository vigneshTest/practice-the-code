package com.suriya;

public class Palintrom {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       String s= "level";
       String r= ""; 
       int n= s.length();
       {
    	   for(int i=n-1; i>=0;i--)
    		   r=r+s.charAt(i);
       }
         System.out.println(r);
       
        if(s.contentEquals(r))
        		 
        		 { 
        	         System.out. println("palindrom");
        	         
        		 }
         else
         { 
        	 System.out.println("not palindrom");
        	 
        	 
        	 
         }
       }
       
	}


